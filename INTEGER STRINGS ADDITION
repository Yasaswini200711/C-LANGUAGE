#include<stdio.h>
#include<string.h>
void main(){
    char s1[20];
    char s2[20];
    int a=0,b=0,c=0,d,e;
    scanf("%s",s1);
    scanf("%s",s2);
    for(int i=0;i<strlen(s1);i++){
        d=s1[i]-'0';
        b=b*10+d;
    }
    for(int i=0;i<strlen(s2);i++){
        e=s2[i]-'0';
        c=c*10+d;
    }
    printf("%d",b+c);
}
